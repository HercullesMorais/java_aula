package principal;

public class Program {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
		int nota1 = 6;
		int nota2 = 5;
		int nota3 = 7;
		int nota4 = 10;
		
		int media = (nota1 + nota2 + nota3 + nota4) /4;
		System.out.println(media);
		
		if (media > 5 && media < 6) {
			System.out.println("Aluno reprovado!");
		}
		
		
		
		

	}

}
